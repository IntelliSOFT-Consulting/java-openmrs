<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="#JAVAPACKAGE#">

	<class name="#JAVACLASSNAME#" table="#MYSQLTABLENAME#">

		<id name="#JAVAIDPROPERTY#" column="#MYSQLIDCOLUMN#">
			<generator class="native" />
		</id>

		<property name="#JAVAPROPERTYNAME#" column="#MYSQLCOLUMNNAME#" type="#HIBERNATETYPE#" />
		
	</class>

</hibernate-mapping>