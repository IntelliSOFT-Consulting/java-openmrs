<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="appointment">
	<class name="Appointment" table="appointment">		
		<id name="appointmentId" column="appointment_id">
			<generator class="native"/>
		</id>
		<property name="patientName" type="string" column="patient_name"/>
		<property name="date" type="date" column="date"/>
	</class>
</hibernate-mapping>
